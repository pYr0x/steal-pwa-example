                                <!--<x-content vm:app:from="../." page="home" title="Home"></x-content>-->
                            <!--{{/case}}-->
                            <!--{{#case "home"}}-->
                                <!--<x-content vm:app:from="../." page="home" title="Home"></x-content>-->
                            <!--{{/case}}-->
                            <!--{{#case "done-autorender"}}-->
                                <!--<x-content vm:app:from="../." page="autorender" title="Done-Autorender"></x-content>-->
                            <!--{{/case}}-->
                        <!--{{/switch}}-->

                    {{else}}
                        Loading...
                    {{/if}}
                </can-import>
    </article>
    <footer class="footer">handmade with StealJS and CanJS</footer>
</div>
</body>
</html>