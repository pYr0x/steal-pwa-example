{
  "name": "steal-pwa-example",
  "version": "0.0.1",
  "description": "PWA with StealJS and CanJS",
  "main": "index.js",
  "scripts": {
    "build": "node build.js",
    "optimize": "node optimize.js"
  },
  "keywords": [
    "StealJS"
  ],
  "author": "Julian Kern",
  "license": "MIT",
  "dependencies": {
    "can-ajax": "^1.0.6",
    "can-component": "^3.3.4",
    "can-define": "^1.4.5",
    "can-fixture": "^1.1.1",
    "can-route": "^3.2.3",
    "can-route-pushstate": "^3.1.1",
    "can-stache": "^3.8.0",
    "can-stache-bindings": "^3.9.0",
    "can-util": "^3.10.6",
    "done-autorender": "^1.3.1",
    "steal": "^1.5.13",
    "steal-css": "^1.3.1",
    "steal-stache": "^3.1.1"
  },
  "steal": {
    "directories": {
      "lib": "src"
    },
    "main": "steal-pwa-example/index.stache!done-autorender/no-zone",
    "plugins": [
      "steal-stache",
      "steal-css",
      "done-autorender"
    ],
    "npmIgnore": [
      "steal-tools"
    ]
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "steal-serviceworker": "^1.0.0",
    "steal-tools": "^1.8.4"
  }
}
